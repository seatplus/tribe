{
    "name": "seatplus/tribe",
    "description": "Base package for any connector that supports syncing of control groups to tribes",
    "keywords": [
        "seatplus",
        "laravel",
        "tribe"
    ],
    "homepage": "https://github.com/seatplus/tribe",
    "license": "MIT",
    "authors": [
        {
            "name": "Felix Huber",
            "email": "felix.a.huber@gmx.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "seatplus/web": "^3.0",
        "seatplus/connector": "^1.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "laravel/pint": "^1.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "nunomaduro/larastan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Seatplus\\Tribe\\": "src",
            "Seatplus\\Tribe\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seatplus\\Tribe\\Tests\\": "tests"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "./vendor/bin/pest",
        "test-coverage": "XDEBUG_MODE=coverage ./vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seatplus\\Tribe\\TribeServiceProvider"
            ],
            "aliases": {
                "Tribe": "Seatplus\\Tribe\\Facades\\Tribe"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
